type Mutation {
  login(nickname: String!, password: String!): LoginResponse!
}

type LoginResponse {
  accessToken: String
}

type Query {
  example1: Person!

  findUser(nickname: String!): User

  protected: Boolean!

  unprotected: Boolean!
}

type Person {
  name: String!

  height: Int!
}

type User {
  id: Int!

  nickname: String!

  password: String!
}

schema {
  query: Query
  mutation: Mutation
}
